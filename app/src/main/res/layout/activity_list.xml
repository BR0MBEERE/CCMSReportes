<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnNewReporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end|bottom"
        android:src="@drawable/add_icon2"
        app:backgroundTint="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="@+id/lista_reportes"
        app:layout_constraintEnd_toEndOf="@+id/lista_reportes" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/lista_reportes"
        android:layout_width="397dp"
        android:layout_height="714dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp" />

    <!--android:src="@drawable/ic_my_icon"
        android:contentDescription="@string/submit"-->

</androidx.constraintlayout.widget.ConstraintLayout>